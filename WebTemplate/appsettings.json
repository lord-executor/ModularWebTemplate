{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UseHttpsRedirection": true,
  "Auth":{
    "Authority": "https://my-authority.net/",
    "Audience": "https://my-application.net",
    "PolicyClaims": {
      "Sample": ["sample"]
    }
  },
  "Cors": {
    "Origins": ["http://localhost:4200"]
  },
  "OTEL_EXPORTER_OTLP_ENDPOINT": "http://my.otel.endpoint:4317",
  "OTEL_EXPORTER_OTLP_HEADERS": "x-otlp-api-key=my.api.key"
}
